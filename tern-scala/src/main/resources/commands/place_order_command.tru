
namespace trumid.poc.example.commands;

type PlaceOrderCommand {

   @Positive
   userId: Int;

   @Positive
   accountId: Int?;
   order: OrderInfo;

   @Positive
   time: Long;

   type OrderInfo {

      @NotBlank
      orderId: String;

      @NotBlank
      symbol: String;

      @Positive
      price: Double;

      @Positive
      quantity: Double;
   }
}

type PlaceOrderResponse {

   @NotBlank
   reason: String?;
   success: Boolean;

   @Positive
   time: Long;
}
