
namespace trumid.ats.cluster.demo.api.events;

type OrderBookSubscribeCommand {

   @Positive
   instrumentId: Int;
}

type OrderBookUpdateEvent {

   @Positive
   instrumentId: Int;

   bids: Order[];
   offers: Order[];

   type Order {

      @Positive
      orderId: Long;

      @Positive
      price: Double;

      @Positive
      quantity: Long;
      changeQuantity: Long;
   }
}
