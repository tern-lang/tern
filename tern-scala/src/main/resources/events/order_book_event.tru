
namespace trumid.poc.example.events;

type OrderBookSubscribeCommand {

   @Positive
   instrumentId: Int;
}

type OrderBookUpdateEvent {

   @Positive
   instrumentId: Int;

   bids: Order[];
   offers: Order[];

   type Order {

      @NotBlank
      orderId: String;

      @Positive
      price: Double;

      @Positive
      quantity: Double;
   }
}
