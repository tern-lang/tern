
namespace foo.bar;

import * from "./other.tru";
import {String, Blah} from "foo/x.tru";

const DEFAULT_VALUE = 0.0;

/**
 * This represents a struct that is generated in to a codec
 * that aligns by data in to a single byte buffer.
 */
type IndexPrice {

   @PositiveOrZero
   price: Double = DEFAULT_VALUE;

   @NotBlank
   symbol: String;
   convention: PriceConvention;

   // inner enum to define price convention
   enum PriceConvention {
      Spread,
      Price
   }
}

