
namespace foo.bar.foo;


union PlaceOrder {
    market: MarketOrder;
    limit: LimitOrder;

    type LimitOrder {

        @PositiveOrZero
        price: Double;

        enum TimeInForce {
            GoodTilCancel,
            GoodForSession
        }
    }

    type MarketOrder {

        @PositiveOrZero
        price: Double;
    }
}